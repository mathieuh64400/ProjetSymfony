{% extends 'base.html.twig' %}

{% block title %}Adresse{% endblock %}

{% block body %}
    <h1>Adresse</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adresse.id }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ adresse.Pays }}</td>
            </tr>
            <tr>
                <th>TypeAdresse</th>
                <td>{{ adresse.typeAdresse ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ adresse.Ville }}</td>
            </tr>
            <tr>
                <th>CodePostal</th>
                <td>{{ adresse.codePostal }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ adresse.Adresse }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('adresse_index') }}">back to list</a>

    <a href="{{ path('adresse_edit', {'id': adresse.id}) }}">edit</a>

    {{ include('adresse/_delete_form.html.twig') }}
{% endblock %}
